@model Dictionary<string, int>

@{
    Layout = null;
    var noItemsModel = new ListNoItemsViewModel("fas fa-dizzy", "You have no positions right now.");
}


@if (!Model.Any())
{
    <partial name="_ListNoItems" model="noItemsModel" />
}
else
{
    <ul class="list-group">
        @foreach (var item in Model)
        {
            <li class="list-group-item d-flex justify-content-between align-items-center">
                @SharedLocalizer[item.Key]
                <span class="badge badge-primary badge-pill">@item.Value</span>
            </li>
        }
    </ul>

}
